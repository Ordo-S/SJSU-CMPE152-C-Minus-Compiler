int fibonacci(int i) {

   if(i == 0) {
      return 0;
   }
	
   if(i == 1) {
      return 1;
   }
   return fibonacci(i-1) + fibonacci(i-2);
}

int  main(void) {
   int a;
   int a;
   int i;
   
   int j;
   j = 0; 
  
   i = 0;
	
   while(i<10) {
   		if(j<2){
   			j = j+1;
   		}
      i= i+1;
   }
   a = fibonacci(i);
 
   return 0;
}